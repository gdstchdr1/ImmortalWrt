config amlogic 'config'
	option enable '1'
	option amlogic_firmware_repo 'https://github.com/gdstchdr1/build_openwrt'
	option amlogic_firmware_tag 's905d'
	option amlogic_firmware_suffix '.img.gz'
	option amlogic_kernel_path 'https://github.com/breakingbadboy/OpenWrt'
	option amlogic_firmware_config '1'
	option amlogic_write_bootloader '0'
	option amlogic_shared_fstype 'ext4'

config settings 'armcpu'
	option maxfreq0 '1800000'
	option governor0 'performance'
	option minfreq0 '1080000'
